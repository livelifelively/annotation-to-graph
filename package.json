    "load": "node --no-warnings --loader ts-node/esm src/load.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0"
  }
}
